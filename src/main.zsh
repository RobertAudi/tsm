function __tsm::main() {
  local cmd="$1"
  [[ "$cmd" == "tsm" ]] && { __tsm::tsm ; return $status }
  if (( ${+__tsm_commands[(r)$cmd]} )); then
    __tsm::"$cmd" "${@:2}"
  else
    __tsm::help
    return 1
  fi
}

